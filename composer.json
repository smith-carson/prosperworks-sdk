{
    "name": "smith-carson/prosperworks-sdk",
    "description": "Unofficial (but pretty decent) SDK for the ProsperWorks CRM API",
    "keywords": ["prosperworks","crm","sdk","api-sdk","guzzle"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://smith-carson.github.io/prosperworks-sdk",
    "authors": [
        {
            "name": "Igor Santos",
            "email": "igorsantos07@gmail.com",
            "homepage": "http://igorsantos.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/smith-carson/prosperworks-sdk/issues",
        "source": "https://github.com/smith-carson/prosperworks-sdk"
    },

    "autoload": {
        "psr-4": {
            "ProsperWorks\\": "src/"
        }
    },

    "minimum-stability": "stable",
    "require": {
        "php": "^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/inflector": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
